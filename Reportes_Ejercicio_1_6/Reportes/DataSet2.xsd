          <DbSource ConnectionRef="ejercicio_1_6_camionesConnectionString (Settings)" DbObjectName="ejercicio_1_6_camiones.dbo.camiones" DbObjectType="Table" GenerateShortCommands="true" GeneratorSourceName="ScalarQueryCamiones_cargas" MethodsParameterType="CLR" Modifier="Public" Name="ScalarQueryCamiones_cargas" QueryType="Scalar" ScalarCallRetval="System.String, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="true" UserGetMethodName="GetDataBy" UserSourceName="ScalarQueryCamiones_cargas">
            <SelectCommand>
              <DbCommand CommandType="Text" ModifiedByUser="true">
                <CommandText>SELECT        cam.patente AS 'Patente', AVG(cam.peso_maximo) AS 'Peso_Maximo', SUM(car.peso) AS 'Peso_de_la_carga'
FROM            camiones AS cam INNER JOIN
                         cargas AS car ON car.patente = cam.patente INNER JOIN
                         tipo_cargas AS tp ON tp.codigo_tipo_carga = car.codigo_tipo_carga INNER JOIN
                         estados AS e ON cam.id_estado = e.id_estado
WHERE        (cam.id_estado &lt;&gt; 4)
GROUP BY cam.patente
ORDER BY 'Patente'</CommandText>
