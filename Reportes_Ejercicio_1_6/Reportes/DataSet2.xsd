                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>select cam.patente'Patente', AVG(cam.peso_maximo)'Peso_Maximo', SUM( car.peso)'Peso_de_la_carga'
 
	 from camiones cam
	 join cargas car
	 on car.patente= cam.patente
	 join tipo_cargas tp
	 on tp.codigo_tipo_carga= car.codigo_tipo_carga
	 join estados e
